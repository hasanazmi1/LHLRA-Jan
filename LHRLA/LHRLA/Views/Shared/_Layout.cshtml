    <script type="text/javascript">
        function SuccessToast(text) {
            $.toast({
                heading: 'Success',
                text: text,
                showHideTransition: 'slide',
                icon: 'success',
                position: 'right',
                color: ''


            })

        };

        function ErrorToast(text) {
            $.toast({
                heading: 'Deleted',
                text: text,
                showHideTransition: 'slide',
                icon: 'error',
                position: 'right',
            })
        };
        function WarningToast(text) {
            $.toast({
                heading: 'Warning',
                text: text,
                showHideTransition: 'slide',
                icon: 'warning',
                position: 'right',
            })
        };
    </script>
