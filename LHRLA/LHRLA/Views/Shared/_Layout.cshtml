    <script src="~/assets/js/vendors.bundle.js" type="text/javascript"></script>
    <script src="~/assets/js/scripts.bundle.js" type="text/javascript"></script>
    <script src="~/assets/js/bootstrap-datetimepicker.js" type="text/javascript"></script>
    <script src="~/assets/js/fullcalendar.bundle.js" type="text/javascript"></script>
    <script src="~/assets/js/basic.js" type="text/javascript"></script>
    <script src="~/assets/js/dashboard.js" type="text/javascript"></script>
    <script src="~/assets/js/dx.all.js" type="text/javascript"></script>
    <script src="~/assets/js/jquery.validate.js"></script>
    <script src="~/assets/js/jquery.additional-methods.js"></script>
